       RowBox[{"Sin", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"\[Omega]$m", "+", "\[Omega]$am"}], ")"}], " ", "t"}], 
        "]"}]}], "\[IndentingNewLine]", "+", " ", 
      RowBox[{
       FractionBox["1", "4"], 
       RowBox[{"R", "[", 
        RowBox[{"0", ",", "0", ",", "z", ",", 
         RowBox[{"\[Omega]$m", "-", "\[Omega]$am"}]}], "]"}], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"\[Omega]$m", "-", "\[Omega]$am"}], ")"}], " ", "t"}], 
        "]"}]}], "+", " ", 
      RowBox[{
