   RowBox[{"Clear", "[", 
    RowBox[{"\[Phi]$rxn", ",", "R", ",", "\[Omega]$m"}], "]"}], ";"}], 
  " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Phi]$rxn", "[", 
    RowBox[{"z_", ",", "t_"}], "]"}], "=", 
   RowBox[{"V$0", " ", 
    RowBox[{"c", "[", 
     RowBox[{"0", ",", "z"}], "]"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"R", "[", 
        RowBox[{"0", ",", "0", ",", "z", ",", "\[Omega]$m"}], "]"}], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"\[Omega]$m", " ", "t"}], "]"}]}], "+", " ", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"R", "[", 
        RowBox[{"1", ",", "0", ",", "z", ",", "\[Omega]$m"}], "]"}], " ", 
       RowBox[{"Sin", "[", 
        RowBox[{"\[Omega]$m", " ", "t"}], "]"}]}], "\[IndentingNewLine]", "+",
       " ", 
      RowBox[{
       FractionBox["1", "4"], 
       RowBox[{"R", "[", 
        RowBox[{"0", ",", "0", ",", "z", ",", 
         RowBox[{"\[Omega]$m", "+", "\[Omega]$am"}]}], "]"}], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"\[Omega]$m", "+", "\[Omega]$am"}], ")"}], " ", "t"}], 
        "]"}]}], "+", " ", 
      RowBox[{
       FractionBox["1", "4"], 
       RowBox[{"R", "[", 
        RowBox[{"1", ",", "0", ",", "z", ",", 
